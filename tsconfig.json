{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "global.d.ts"],
  "exclude": [
    "**/node_modules",
    "**/Pods",
    
    "navigation/**",
    "screens/**"
  ]
}
